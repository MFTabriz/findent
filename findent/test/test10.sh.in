#!/bin/sh
rc=0
prog="  program prog
  continue"

for flag in -lastindent --last_indent ; do
   a=`echo "$prog" | ../src/findent $flag -ifree -Ia -i3`
   if [ "$a" = "5" ]; then
      echo "$flag : works OK"
   else
      echo "$flag : works NOT OK"
      echo "expected: 5"
      echo "got     : $a"
      echo "program is:"
      echo "$prog"
      rc=`expr $rc + 1`
   fi
done
exit $rc
